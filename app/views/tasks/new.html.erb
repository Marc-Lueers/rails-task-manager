<div class="container">
  <h1>New task</h1>
  <%= form_for(@task) do |f| %>

      <strong><%= f.label :title %></strong>
      <%= f.text_field :title, class: 'form-control' %>

      <strong><%= f.label :details %></strong>
      <%= f.text_field :details, class: 'form-control' %>
   
 <%= f.submit class: 'btn btn-primary' %>
  <% end %>
   <<span><%= link_to "Back to list", tasks_path %>
    </div>
